<!DOCTYPE html>
<html>
<head>
  <title>TCHpwatermarkapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<% if current_user %>
   Signed in as <%= current_user.username %> | <%= link_to "Logout", '/users/signout' %>
   <% else %>
            <%= link_to 'Login', '/users/login' %> | <%= link_to 'Signup', '/users/new' %>
   <% end %>

<%= yield %>

</body>
</html>
